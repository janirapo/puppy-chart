{
    "name": "puppy-chart",
    "version": "1.0.0",
    "description": "This is a project for a school course called Full stack-websovelluskehitys at University of Helsinki",
    "directories": {
        "doc": "docs"
    },
    "main": "index.js",
    "scripts": {

        "start"        : "npm-run-all --parallel watch:server watch:build",
        "watch:build"  : "./node_modules/.bin/webpack -w",
        "watch:server" : "nodemon src/server/index.js",
        "webpack": "./node_modules/.bin/webpack -w",
        "test": "mocha --compilers babel-core/register ./test/*test.js",
        "lint-staged": {
            "linters": {
                "src/**/*.{js,jsx,json,css}": [
                    "prettier --single-quote --tab-width 4 --print-width 120 --trailing-comma all --write",
                    "git add"
                ]
            }
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/janirapo/puppy-chart.git"
    },
    "author": "Jani Rapo",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/janirapo/puppy-chart/issues"
    },
    "homepage": "https://github.com/janirapo/puppy-chart#readme",
    "keywords": [],
    "dependencies": {
        "axios": "^0.18.0",
        "babel-preset-env": "^1.6.1",
        "cps": "^1.0.2",
        "express": "^4.16.3",
        "moment": "^2.22.0",
        "mysql": "^2.15.0",
        "node-mysql": "^0.4.2",
        "npm-run-all": "^4.1.2",
        "openrecord": "^2.0.0-rc10",
        "prettier": "^1.11.1",
        "prop-types": "^15.6.1",
        "react": "^16.3.0",
        "react-dom": "^16.3.0",
        "react-icons": "^2.2.7",
        "react-redux": "^5.0.7",
        "redux": "^3.7.2",
        "redux-thunk": "^2.2.0"
    },
    "devDependencies": {
        "babel": "^6.23.0",
        "babel-core": "^6.26.0",
        "babel-loader": "^7.1.4",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "chai": "^4.1.2",
        "css-loader": "^0.28.11",
        "enzyme": "^3.3.0",
        "mocha": "^5.0.5",
        "node-sass": "^4.8.3",
        "redux-devtools": "^3.4.1",
        "sass-loader": "^6.0.7",
        "style-loader": "^0.20.3",
        "webpack": "^4.4.1",
        "webpack-cli": "^2.0.13"
    }
}
