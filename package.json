{
    "name": "puppy-chart",
    "version": "1.0.0",
    "engines": {
        "node": "8.9.1"
    },
    "private": true,
    "description": "This is a project for a school course called Full stack-websovelluskehitys at University of Helsinki",
    "directories": {
        "doc": "docs"
    },
    "main": "index.js",
    "scripts": {
        "start": "npm-run-all --parallel watch:server watch:build",
        "watch:build": "./node_modules/.bin/webpack -w",
        "watch:server": "node src/server/index.js",
        "webpack": "./node_modules/.bin/webpack -w",
        "test": "mocha --compilers babel-core/register ./test/*test.js",
        "precommit": "lint-staged"
    },
    "lint-staged": {
        "linters": {
            "src/**/*.{js,jsx,json,css}": [
                "prettier --single-quote --tab-width 4 --print-width 120 --trailing-comma all --write",
                "git add"
            ]
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/janirapo/puppy-chart.git"
    },
    "author": "Jani Rapo",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/janirapo/puppy-chart/issues"
    },
    "homepage": "https://github.com/janirapo/puppy-chart#readme",
    "keywords": [],
    "dependencies": {
        "axios": "^0.18.0",
        "babel-preset-env": "^1.6.1",
        "classnames": "^2.2.5",
        "compression": "^1.7.2",
        "cors": "^2.8.4",
        "crypto": "^1.0.1",
        "errorhandler": "^1.5.0",
        "express": "^4.16.3",
        "express-jwt": "^5.3.1",
        "fs": "0.0.1-security",
        "helmet": "^3.12.0",
        "i18next": "^11.2.3",
        "jsonwebtoken": "^8.2.1",
        "jwt-decode": "^2.2.0",
        "moment": "^2.22.1",
        "mysql2": "^1.5.3",
        "node-mysql": "^0.4.2",
        "npm-run-all": "^4.1.2",
        "passport": "^0.4.0",
        "passport-local": "^1.0.0",
        "path": "^0.12.7",
        "prettier": "^1.11.1",
        "prop-types": "^15.6.1",
        "react": "^16.3.0",
        "react-day-picker": "^7.1.6",
        "react-dom": "^16.3.0",
        "react-i18next": "^7.6.0",
        "react-icons": "^2.2.7",
        "react-modal": "^3.3.2",
        "react-redux": "^5.0.7",
        "redux": "^4.0.0",
        "redux-form": "^7.3.0",
        "redux-thunk": "^2.2.0",
        "sequelize": "^4.37.6"
    },
    "devDependencies": {
        "babel": "^6.23.0",
        "babel-core": "^6.26.0",
        "babel-loader": "^7.1.4",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "chai": "^4.1.2",
        "css-loader": "^0.28.11",
        "enzyme": "^3.3.0",
        "html-webpack-plugin": "^3.2.0",
        "mocha": "^5.0.5",
        "node-sass": "^4.8.3",
        "redux-devtools": "^3.4.1",
        "sass-loader": "^7.0.1",
        "style-loader": "^0.21.0",
        "webpack": "^4.4.1",
        "webpack-cli": "^2.0.13"
    }
}
